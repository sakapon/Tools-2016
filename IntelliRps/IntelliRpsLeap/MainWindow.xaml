<Window x:Class="IntelliRpsLeap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IntelliRpsLeap"
        mc:Ignorable="d"
        Title="Intelli-RPS" Height="350" Width="525" WindowState="Maximized" FontSize="32">
    <Window.DataContext>
        <local:AppModel/>
    </Window.DataContext>
    <Grid>
        <TextBlock Text="{Binding HandShape.Value}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Window>
